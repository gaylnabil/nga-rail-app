<p style="color: green"><%= notice %></p>

<h1>Users</h1>

<div id="users">
<%# each loop is a Rails syntax that includes _from.html.erb %>

  <% @users.each do |user| %>
    <%= render user %>
    <p>
      <%= link_to "Show this user", user %>
    </p>
  <% end %>
</div>

<%= link_to "New user", new_user_path %>
